# ===== REQUIRED =====
BOT_TOKEN=your_bot_api_token
OWNER_ID=your_user_id

# ===== OPTIONAL =====
# API Credentials (for userbot features)
API_ID=your_api_id
API_HASH=your_api_hash

# Additional Admins (comma separated)
SUDO_USERS=sudo_user_ids #example=(987654321,876543210)
DEV_USERS=devloper_id

# Database
DATABASE_URL=sqlite:///rose_bot.db
MONGO_URI=mongodb://localhost:27017/rose_bot
REDIS_URL=redis://localhost:6379/0

# Logging
LOG_CHANNEL=-1001234567890
ERROR_LOG=-1001234567891

# Support Chats
SUPPORT_CHAT=@RoseSupportChat
UPDATE_CHANNEL=@RoseUpdateChannel
SUPPORT_CHANNEL=@RoseSupportChannel

# Bot Settings
WORKERS=8
TIMEZONE=UTC
ALLOW_EXCL=true
DEL_CMDS=true

# Federation IDs
FED_IDS=fed1,fed2,fed3

# AntiSpam Service (cas/antispam/sentinel)
ANTISPAM_SERVICE=cas
